<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.doeiqts.rollerderbyplaybook"
    android:versionCode="3"
    android:versionName="1.2" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:icon="@drawable/rollerderbyplaybook"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity android:screenOrientation="landscape" android:configChanges="orientation|keyboardHidden"
            android:name=".RolleryDerbyPlaybookActivity"
            android:label="@string/title_activity_rollery_derby_playbook" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>